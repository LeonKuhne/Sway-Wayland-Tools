{
	"default_port": 0,
	"timeout": 250,
	"displays": ["DP-1", "DVI-D-1", "DVI-D-2", "HDMI-A-1"],
	"bindings": [
		{
			"A#-2": "SingleBind('playerctl play')",
			"B-2": "SingleBind('playerctl pause')",
			"D-1": "SingleBind('playerctl next')",
			"C#-1": "SingleBind('playerctl previous')",
			"C1": "ControllerBind(f'pactl set-sink-volume @DEFAULT_SINK@ %s')"
		},
		{
		        "C2": "KillOnReleaseBind('spotify', 2, 'music')",
	        	"A2": "KillOnReleaseBind('firefox-nightly', 0, 'internet')",
		        "B2": "KillOnReleaseBind('discord', 2, 'discord')",
		        "G2": "ToggleBind('termite', 1, 'code')",
		        "D2": "ToggleBind('htop')",
			"E2": "EnvBind('bind_nethogs')",
			"F2": "EnvBind('bind_iftop')",
			"D3": "SingleBind('', 'code')",
			"B3": "SingleBind('', 'music')",
			"A3": "SingleBind('', 'internet')",
			"C4": "SingleBind('', 'discord')",
			"D4": "SingleBind('~/.tools/image_today.sh')"
		}
	]
}

